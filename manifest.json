{
  "manifest_version": 2,

  "name": "code block selector",
  "description": "This extension  allows code block selecting",
  "version": "1.0",

  "permissions": [
    "activeTab",
    "clipboardRead",
    "clipboardWrite"
  ],
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["jquery.min.js", "clipboard.js", "codeblocks.js"]
    }
  ]
}
